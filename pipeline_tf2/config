export ADDEN2_DIR=/hps/nobackup/research/gerstung/josegcpa/data/ADDEN_SVS
export ADDEN2_OUTPUT_DIR=$RESULTS_DIR/ADDEN_SVS
export ADDEN2_FMT=svs

# paths to checkpoints
export QC_CKPT=/hps/research/gerstung/josegcpa/projects/01IMAGE/quality-net/checkpoints/quality-net/quality-net-25
